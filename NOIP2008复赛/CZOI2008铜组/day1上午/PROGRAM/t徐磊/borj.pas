var a:array[1..10]of int64;
    i,n,j:longint;
begin
  assign(input,'borj.in');reset(input);
  assign(output,'borj.out');rewrite(output);
  readln(n);
  j:=10;
  i:=1;
  while n div j<>0 do begin i:=i+1;j:=j*10;end;
  i:=i-2;
  a[1]:=9;
  a[2]:=99-a[1]+1;
  a[3]:=999-a[1]-a[2]+1;
  a[4]:=9999-a[1]-a[2]-a[3]+1;
  a[5]:=99999-a[1]-a[2]-a[3]-a[4]+1;
  a[6]:=999999-a[1]-a[2]-a[3]-a[4]-a[5]+1;
  a[7]:=9999999-a[1]-a[2]-a[3]-a[4]-a[5]-a[6]+1;
  a[8]:=99999999-a[1]-a[2]-a[3]-a[4]-a[5]-a[6]-a[7]+1;
  a[9]:=999999999-a[1]-a[2]-a[3]-a[4]-a[5]-a[6]-a[7]-a[8]+1;
  a[10]:=999999999-a[1]-a[2]-a[3]-a[4]-a[5]-a[6]-a[7]-a[8]-a[9]+1;
  if n<10 then n:=n*1;
  if (n>=10)and(n<100) then n:=a[1]*1+(n-a[1])*2;
  if (n>=100)and(n<1000) then n:=a[1]*1+a[2]*2+(n-a[1]-a[2])*3;
  if (n>=1000)and(n<10000) then n:=a[1]*1+a[2]*2+a[3]*3+(n-a[1]-a[2]-a[3])*4;
  if (n>=10000)and(n<100000) then n:=a[1]*1+a[2]*2+a[3]*3+a[4]*4+(n-a[1]-a[2]-a[3]-a[4])*5;
  if (n>=100000)and(n<1000000) then n:=a[1]*1+a[2]*2+a[3]*3+a[4]*4+a[5]*5+(n-a[1]-a[2]-a[3]-a[4]-a[5])*6;
  if (n>=1000000)and(n<10000000) then n:=a[1]*1+a[2]*2+a[3]*3+a[4]*4+a[5]*5+a[6]*6+(n-a[1]-a[2]-a[3]-a[4]-a[5]-a[6])*7;
  if (n>=10000000)and(n<100000000) then n:=a[1]*1+a[2]*2+a[3]*3+a[4]*4+a[5]*5+a[6]*6+a[7]*7+(n-a[1]-a[2]-a[3]-a[4]-a[5]-a[6]-a[7])*8;
  writeln(n+i);
  close(input);
  close(output);
end.
